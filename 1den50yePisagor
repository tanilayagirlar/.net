static void Main(string[] args)
        {
            for (int i = 1; i <= 50; i++)
            {
                for (int j = 1; j <= 50; j++)
                {

                    for (int k = 1; k <= 50; k++)
                    {
                        if (i < j && (i * i + j * j) == k * k)
                        {
                            Console.WriteLine(i + "-" + j + "-" + k);
                        }
                    }
                }
            }
        }
